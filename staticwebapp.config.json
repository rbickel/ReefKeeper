{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/_expo/*", "/favicon.png", "/favicon.ico"]
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=0, must-revalidate"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".mjs": "application/javascript"
  }
}
